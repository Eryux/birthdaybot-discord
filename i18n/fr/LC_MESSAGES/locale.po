# Commands help and usage
msgid "Display help for given command.\nUsage: help (<command>)"
msgstr "Affiche l'aide pour une commande donnée.\nUtilisation: help (<commande>)"

msgid "Set birthday for given member.\nUsage: birthday <date> @<member>"
msgstr "Défini l'anniversaire pour d'un membre.\nUtilisation: birthday <date> @<membre>"

msgid "Set your birthday or birthday for a given member.\nUsage: birthday <date> (@<member>)"
msgstr "Défini votre anniversaire ou l'anniversaire pour d'un membre.\nUtilisation: birthday <date> (@<membre>)"

msgid "Remove birthday of a given member.\nUsage: remove @<member>"
msgstr "Supprime l'anniversaire d'un membre.\nUtilisation: remove @<membre>"

msgid "Remove your birthday or remove birthday of a given member.\nUsage: remove (@<member>)"
msgstr "Supprime vrote date d'anniversaire ou l'anniversaire d'un membre.\nUtilisation: remove (@<membre>)"

msgid "Display birthday for a given member.\nUsage: of @<member>"
msgstr "Affiche l'anniversaire d'un membre.\nUtilisation: of @<membre>"

msgid "Allow given role to use the bot.\nUsage: role add <role>"
msgstr "Autorise un role à utiliser le bot.\nUtilisation: role add <role>"

msgid "Remove right to use bot for a given role.\nUsage: role remove <role>"
msgstr "Supprime le droit d'utiliser le bot pour un role.\nUtilisation: role remove <role>"

msgid "Allow given role to manage the bot.\nUsage: admin-role add <role>"
msgstr "Autorise un role à gérer le bot.\nUtilisation: admin-role add <role>"

msgid "Remove right to manage bot from given role.\nUsage: admin-role remove <role>"
msgstr "Supprime le droit de gérer le bot pour un role.\nUtilisation: admin-role remove <role>"

msgid "Set server timezone.\nUsage: set-timezone <timezone>"
msgstr "Défini le fuseau horraire du serveur.\nUtilisation: set-timezone <fuseau>"

msgid "Set server date format used to ask people birthday.\nUsage: set-dateformat <format>"
msgstr "Défini le format de date utilisé pour demandé l'anniversaire des personnes.\nUtilisation: set-dateformat <format>"

msgid "Set server language.\nUsage: set-lang <lang>"
msgstr "Défini la langue utilisé par le bot.\nUtilisation: set-lang <lang>"

msgid "Set hour at bot celebrate members birthday on server.\nUsage: set-celebrate-hour <hour>"
msgstr "Défini l'heure quand le bot célébre les anniversaires sur le serveur.\nUtilisation: set-celebrate-hour <hour>"

msgid "Set channel where bot send birthday message.\nUsage: set-celebrate-channel #<channel>"
msgstr "Défini le salon où le bot envoi le message d'anniversaire.\nUtilisation: set-celebrate-channel #<salon>"

msgid "Set birthday message.\nUsage: set-birthday-message <message>\nUse {member} and {age} tag for displaying member name and member age in message."
msgstr "Défini le message d'anniversaire.\nUtilisation: set-birthday-message <message>\nUtiliser {member} et {age} pour afficher le nom et l'age du membre dans le message."

msgid "Define if we keep member birthday even if there aren't on the server.\nUsage: set-keep-onleave <0/1>"
msgstr "Défini si on garde l'anniversaire des membres même si ils ont quittés le serveur.\nUtilisation: set-keep-onleave <0/1>"

msgid "Set special role for members having birthday.\nUsage: set-birthday-role <role>"
msgstr "Défini un role pour l'anniversaire des membres.\nUtilisation: set-birthday-role <role>"

msgid "Allow given channel to be used for bot command channel from members.\nUsage: channel add #<channel>"
msgstr "Autorise un salon à être utilisé pour les commandes du bot par les membres.\nUtilisation: channel add #<salon>"

msgid "Remove channel from allowed bot command channel.\nUsage: channel remove #<channel>"
msgstr "Supprime l'autorisation d'utilisé un salon pour le commande bot.\nUtilisation: channel remove #<channel>"

msgid "Display fake birthday message.\nUsage: birthday-preview"
msgstr "Affiche un faux message d'anniversaire.\nUtilisation: birthday-preview"

msgid "Display bot details.\nUsage: status"
msgstr "Affiche les informations sur le bot.\nUtilisation: status"

msgid "Force the bot to wish members birthdays.\nUsage: force-wish"
msgstr "Force le bot à souhaiter les anniversaires des membres.\nUtilisation: force-wish"

msgid "Stop the bot program.\nUsage: bot-stop"
msgstr "Arrête le programme du bot.\nUtilisation: bot-stop"